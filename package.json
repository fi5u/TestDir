{
  "name": "web",
  "version": "2.0.0",
  "private": true,
  "browserslist": [
    "last 2 versions",
    ">0.3%",
    "not ie > 0",
    "not ie_mob > 0"
  ],
  "scripts": {
    "build": "next build",
    "dep:dev": "vercel --env NEXT_PUBLIC_ENVIRONMENT=stage",
    "dep:prod": "cli-confirm \"Deploy to PRODUCTION and bypass tests, are you sure?\" && vercel --prod",
    "dev": "next dev",
    "dev:stage": "npm run dev:stage:dev",
    "dev:stage:alfa": "USE_STAGE_SERVER=alfa next dev",
    "dev:stage:beta": "USE_STAGE_SERVER=beta next dev",
    "dev:stage:dev": "USE_STAGE_SERVER=dev next dev",
    "lint": "next lint",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged",
    "start": "next start",
    "test": "jest -o",
    "test-all": "jest"
  },
  "dependencies": {
    "@greatsumini/react-facebook-login": "^3.2.1",
    "@react-hook/resize-observer": "^1.2.6",
    "aes-everywhere": "^1.0.0",
    "animejs": "^3.2.1",
    "appboy-web-sdk": "^3.5.1",
    "classnames": "^2.3.2",
    "confetti-js": "^0.0.18",
    "cookie": "^0.5.0",
    "copy-text-to-clipboard": "^3.0.1",
    "downshift": "^6.1.12",
    "error-stack-parser": "^2.1.4",
    "hls.js": "^1.2.4",
    "iron-session": "^6.2.1",
    "lazysizes": "^5.3.2",
    "locale": "^0.1.0",
    "next": "^12.3.1",
    "next-localization": "^0.12.0",
    "react": "^18.2.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.2.0",
    "react-expanding-textarea": "^2.3.6",
    "react-full-screen": "^1.1.1",
    "react-imgix": "^9.5.4",
    "request-ip": "^3.3.0",
    "stacktrace-js": "^2.0.2",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/animejs": "^3.1.5",
    "@types/locale": "^0.1.1",
    "@types/react": "^18.0.21",
    "@types/react-imgix": "^9.5.0",
    "@types/request-ip": "^0.0.37",
    "cli-confirm": "^1.0.1",
    "eslint": "^8.24.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.1.2",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.6.4"
  }
}
